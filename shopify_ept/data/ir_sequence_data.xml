<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <data noupdate="1">

        <!-- Sequences for product data queue-->
        <record id="seq_product_queue_data" model="ir.sequence">
            <field name="name">Shopify Product Queue Data Sequence</field>
            <field name="code">shopify.product.data.queue.ept</field>
            <field name="prefix">PDQ</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for order data queue-->
        <record id="seq_order_queue_data" model="ir.sequence">
            <field name="name">Shopify Order Data Queue Sequence</field>
            <field name="code">shopify.order.data.queue.ept</field>
            <field name="prefix">ODQ</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for customer data queue-->
        <record id="seq_synced_shopify_customer_sequence" model="ir.sequence">
            <field name="name">Shopify Customer Data Queue Sequence</field>
            <field name="code">shopify.customer.data.queue.ept</field>
            <field name="prefix">CDQ</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for payout report-->
        <record id="seq_import_payout_report" model="ir.sequence">
            <field name="name">Shopify Payout Report Sequence</field>
            <field name="code">shopify.payout.report.ept</field>
            <field name="prefix">PTR</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for Export Stock queue-->
        <record id="seq_export_stock_queue" model="ir.sequence">
            <field name="name">Shopify Export Stock Queue Sequence</field>
            <field name="code">shopify.export.stock.queue.ept</field>
            <field name="prefix">ESQ</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
